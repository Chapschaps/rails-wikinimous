<h1>New article</h1>

<%= form_for @article do |f| %>
 <p></p> <%= f.label :title %>
 <p></p> <%= f.text_field :title %>

 <p></p> <%= f.label :content %>
 <p></p> <%= f.text_field :content %>

<p></p>  <%= f.submit("Create article") %>
<% end %>

<p><%= link_to "↩️ Back to list", articles_path %></p>
